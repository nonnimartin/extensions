{
  "manifest_version": 2,

  "name": "Click for Duplicate Form Fields",
  "description": "Auto Complete Duplicate Form Fields",
  "version": "1.0",

  "icons": {
    "48": "icon.png"
  },

  "permissions": [
    "tabs", "<all_urls>", "https://*/*", "http://*/*"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

    "content_scripts": [ {
    "js": ["jquery-3.1.1.js", "duplicate.js"],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
