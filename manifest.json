{
  "manifest_version": 2,

  "name": "Click for Duplicate Form Fields",
  "description": "Auto Complete Duplicate Form Fields",
  "version": "1.0",

  "icons": {
    "48": "icon.png"
  },

  "permissions": [
    "https://flexerasoftware.my.salesforce.com/*/*"],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["jquery-3.1.1.js", "background.js"],
    "persistent": false
  },

    "content_scripts": [ {
    "js": ["jquery-3.1.1.js", "duplicate.js"],
    //"run_at": "document_start",
    "matches": [ "<all_urls>"]
  }]
}
